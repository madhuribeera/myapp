<%- include layouts/header.ejs %>

        <div class="page-data">
         <div class="data-btn">
           <button onClick="addCategeory();">+ Add</button>
         </div>
         <div class="data-table">
            <table border="1" cellpadding="7" cellspacing="7">
                <tr>
                    <th width="50px">No</th>
                    <th>Name</th>
                     <th>Description</th>
                    <th>Status</th>
                   
                    <th width="120px">Action</th>
                </tr>
                               
                <% if(data.length){ 
                                
                 for(var i = 0;i < data.length;i++) { %>
                 
                <tr>
                    <td><%=(i+1)%></td>
                    <td><%=data[i].Name%></td>
                    <td><%=data[i].Description%></td>
                     <td><%=data[i].Status%></td>
                     
                     


                   
                    <td>
                        <a class="a-inside edit" href="../Categeory/edit/<%=data[i].No%>">Edit</a>
                       
                        <a class="a-inside delete" href="../Categeory/delete/<%=data[i].No%>">Delete</a>
                       
                    </td>
                </tr>
            <% }
            
             }else{ %>
                 <tr>
                    <td colspan="3">No user</td>
                 </tr>
            <% } %>
                
                              
                
            </table>
         </div>
        </div>
        
<%- include layouts/footer.ejs %>
